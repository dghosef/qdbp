unify := {that with| 
  (#True{})
    True? [that]
    False? [with!.].
}
is_string := {that|
  unify! that with: {$_qdbp_empty_string()}.
}
{that |
  {
    Val[that]
    Print[
      _ := $_qdbp_print_string_int(this Val.)
      {}
    ]
    ++[that|
      new_val := $_qdbp_concat_string(this Val. is_string! (that Val).)
      {
        this
        Val[new_val]
      }
    ]
  }
}